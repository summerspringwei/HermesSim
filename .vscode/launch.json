{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: model/main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/model/main.py",
            "args": [
                "--inputdir",
                "./dbs/",
                "--config",
                "./model/configures/e02_repr.json",
                "--dataset=one"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python Debugger: preprocessing_pcode.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/preprocess/preprocessing_pcode.py",
            "args": [
                "--freq-mode",
                "-f",
                "pkl",
                "-s",
                "Dataset-1_testing",
                "-i",
                "dbs/Dataset-1/features/testing/pcode_raw_Dataset-1_testing/",
                "-o",
                "dbs/xcw_preprocessing_Dataset-1_testing"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python Debugger: pcode_lifter.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/lifting/pcode_lifter.py",
            "args": [
                "--cfg_summary",
                "./dbs/Dataset-1/cfg_summary/testing",
                "--output_dir",
                "./dbs/Dataset-1/features/testing/xcw_pcode_raw_Dataset-1_testing",
                "--graph_type",
                "ALL",
                "--verbose",
                "1",
                "--nproc",
                "1"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}